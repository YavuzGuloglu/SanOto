    /* #05C46B #036C3B  */
    body {
        background-color: #05C46B;
    }
</style>


<!-- Breadcrumb -->
<div class="breadcrumbs overlay" style="background-image:url('/eduland-master/images/fun-fact-bg.jpg')">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-12 ">

                <h2>Parola Sıfırlama</h2>
            </div>
            <div class="col-lg-6 col-md-6 col-12">
                <ul class="bread-list">
                    <ul class="bread-list" style="background-color:white">
                        <li>
                            <a href="@Url.Action("Index","Home")"><img src="~/eduland-master/images/logo3.fw.png" alt="AnaSayfa"></a>
                        </li>
                    </ul>
                </ul>
            </div>
        </div>
    </div>
</div>
<!--/ End Breadcrumb -->


<div class="container ">
    <div class="card card-login mx-auto mt-5 col-sm-5">
        <div class="card-header">
            <section class="page-title">
                <h2>Parola Sıfırlama</h2>
                @ViewBag.Uyari
            </section>
        </div>

        <div class="card-body">
            @using (Html.BeginForm("ParolamiUnuttum", "Account", FormMethod.Post))
            {

                @Html.AntiForgeryToken()


                <form action="/Account/ParolamiUnuttum" method="post">
                    <div class="form-group">
                        <label for="email" style="font-size:large"><b>E-Posta</b></label>
                        <input type="email" class="form-control" required="required" style="height:50px" autofocus="autofocus" name="k_eposta" id="k_eposta" placeholder="Mail Adresiniz">
                    </div>
                    <!--end form-group-->
                    <div class="form-group">
                        <label for="password" style="font-size:large"><b>Yeni Parola</b></label>
                        <input type="password" class="form-control" required="required" style="height:50px" name="k_sifre" id="k_sifre" placeholder="Şifreniz">
                    </div>
                    <br />
                  
                    @ViewBag.Message

                    <div class="form-group center">
                        <input type="submit" class="btn btn-primary btn-block" name="ParolamiUnuttum" id="ParolamiUnuttum">
                        @* <button type="submit" class="btn btn-block ">P a r o l a &nbsp;&nbsp;  Y e n i l e</button>*@
                    </div>
                    <!--end form-group-->

                </form>
            }
            <br />
            @if (ViewBag != null)
            {
                <p style="color:red">@ViewBag.Mesaj</p>
            }
            <hr>

            <div class="registration">

                Hesabınız Varsa.
                <a class="" href="@Url.Action("Login", "Account")">
                    Giriş Yap
                </a>
            </div>


        </div>


    </div>
</div>
<br />
<br />

