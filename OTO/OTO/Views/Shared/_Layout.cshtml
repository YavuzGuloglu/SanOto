                                        <li class="active"><a href="@Url.Action("Index","Home")"><i class="fa fa-home"></i>Anasayfa</a></li>

                                        @Html.Partial("_LoginPartial")
                                    </ul>
                                    <!-- End Main Menu -->
                                </div>
                            </nav>
          
